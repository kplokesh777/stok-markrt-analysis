import pandas as pd import matplotlib.pyplot as plt 
AMZN=pd.read_csv("D:\python\AMZN.csv") 
AAPL=pd.read_csv("D:\python\AAPL.csv") 
GOOGL=pd.read_csv("D:\python\GOOGL.csv") 
AMZN.describe() 
AAPL.describe() 
GOOGL.describe() 
AAPL.set_index('Date',inplace=True) 
AMZN.set_index('Date',inplace=True) 
GOOGL.set_index('Date',inplace=True) AAPL["Close"].plot() plt.title("APPLE STOCKS") plt.show() 
AMZN["Close"].plot() plt.title("AMAZON STOCKS") plt.show() 
GOOGL["Close"].plot() plt.title("GOOGLE STOCKS") plt.show() 
AAPL["Close"].plot(label="APPLE Close") 
AMZN["Close"].plot(label="AMAZON Close") GOOGL["Close"].plot(label="GOOGLE Close") plt.title("MNC STOCKS CLOSING") plt.ylabel("STOCKS") plt.show() 
